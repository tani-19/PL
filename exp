for row in data: 
print ("FIRST_NAME = ", row[0]) 
print ("LAST_NAME = ", row[1]) 
print ("AGE = ", row[2]) 
print ("SEX = ", row[3]) 
print ("INCOME = ", row[4]) 
conn.execute("DELETE FROM EMPLOYEE11 WHERE AGE > '%d'" % (40)) 
conn.commit() 
