print ("LAST_NAME = ", row[1]) 
print ("AGE = ", row[2]) 
print ("SEX = ", row[3]) 
print ("INCOME = ", row[4]) 
conn.execute("UPDATE EMPLOYEE11 SET AGE = AGE + 1 WHERE SEX = '%c'" % ('M')) 
conn.commit() 
